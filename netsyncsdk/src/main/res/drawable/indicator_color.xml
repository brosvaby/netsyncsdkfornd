<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <solid
        android:color="#66000000"/>
    <stroke
        android:width="4dp"
        android:color="#66000000"
        android:dashWidth="4dp"
        android:dashGap="2dp" />
    <padding
        android:left="1.0dp"
        android:top="1.0dp"
        android:right="1.0dp"
        android:bottom="1.0dp" />
    <corners android:radius="3.0dp" />
    <gradient
        android:startColor="#66000000"
        android:endColor="#66000000"
        android:angle="270"/>
</shape>